{
  "type": "delphi deploy config",
  "version": 1,
  "actions": [
    {
      "type": "copy",
      "src": "site/index.html",
      "dst": "/var/www/html/forecast/index.html",
      "add-header-comment": true
    },
    {
      "type": "copy",
      "src": "site/style/index.css",
      "dst": "/var/www/html/forecast/style/index.css",
      "add-header-comment": true
    },
    {
      "type": "copy",
      "src": "site/style/plot.css",
      "dst": "/var/www/html/forecast/style/plot.css",
      "add-header-comment": true
    },
    {
      "type": "copy",
      "src": "site/js/plot.js",
      "dst": "/var/www/html/forecast/js/plot.js",
      "add-header-comment": true
    },
    {
      "type": "copy",
      "src": "site/img/delphi_forecast.png",
      "dst": "/var/www/html/forecast/img/delphi_forecast.png",
      "add-header-comment": false
    }
  ]
}
